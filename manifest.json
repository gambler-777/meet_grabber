{
    "name": "__MSG_name__",
    "description": "__MSG_description__",
    "version": "1.2",
    "default_locale": "en",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
